<h1 class="text-center">Available Cabs</h1>
<div class="container">
    {{#if profile.role}}
    <a href="/addCab">
    <button class="btn btn-primary">Add Cab</button>
    </a>
    {{/if}}
    <table class="table table-striped mt-4">
        <tr>
            <th>Cab name</th>
            <th>Description</th>
            <th>Capacity</th>

            
        </tr>
       {{#if profile.role}}
       {{#each cabDetails}}
       <tr>
         <td>{{this.dataValues.cabName}}</td>
         <td>{{this.dataValues.cabDescription}}</td>
         <td>{{this.dataValues.cabTotalCapacity}}</td>
         <td>
            <a href="editCabDetails/{{this.dataValues.cabNo}}">Edit</a>|
            <a href="deleteCabDetails/{{this.dataValues.cabNo}}">Delete</a>
         </td>
       </tr>
        {{/each}}

        {{else}} 
        {{#each cabDetails}}
                <tr>
            <td>{{this.dataValues.cabName}}</td>
            <td>{{this.dataValues.cabDescription}}</td>
            <td>{{this.dataValues.cabTotalCapacity}}</td>
            <td>
                <a href="/booking/{{this.dataValues.cabNo}}">Book now</a>
            </td>
        </tr>

        {{/each}}
       {{/if}}
    </table>
</div>
